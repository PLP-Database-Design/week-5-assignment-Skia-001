<!-- 1. Retrieve all patients
Create a GET endpoint that retrieves all patients and displays their: -->


<table>
    <thead>
        <tr>
            <th>Patient ID</th>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Date Of Birth</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row =>{%>
        <tr>
            <td> <%= row.patient_id %> </td>
            <td> <%=  row.first_name %> </td>
            <td> <%=  row.last_name %> </td>
            <td> <%=  row.date_of_birth %> </td>
        </tr>
       <%});%>
    </tbody>
</table>
