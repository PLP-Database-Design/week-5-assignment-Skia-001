<!-- 2. Retrieve all providers
Create a GET endpoint that displays all providers with their: -->


<table>
    <thead>
        <tr>
            <th>First Name</th>
            <th>Last Name</th>
            <th>Provider Specialty</th>
        </tr>
    </thead>
    <tbody>
        <% results.forEach(row =>{%>
        <tr>
            <td> <%=  row.first_name %> </td>
            <td> <%=  row.last_name %> </td>
            <td> <%=  row.provider_specialty %> </td>
        </tr>
       <%});%>
    </tbody>
</table>